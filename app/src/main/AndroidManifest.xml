<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.relevamiento">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GrabadoraAudio" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".AyudaPlanilla" android:configChanges="screenSize|orientation" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".CambiarDiagramas" android:configChanges="screenSize|orientation" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".DiagramaCompleto" android:configChanges="screenSize|orientation" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".Planilla" android:configChanges="screenSize|orientation" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".Marcar" android:configChanges="screenSize|orientation" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".Principal" android:configChanges="screenSize|orientation"></activity>
        <activity android:name=".CrearCargarProyecto" android:configChanges="screenSize|orientation" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".ListadoProyectos" android:configChanges="screenSize|orientation" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".MainActivity" android:configChanges="screenSize|orientation" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>